class Boton:
        def __init__(self, posx1, posx2, posy1, posy2):
                self.posx1 = posx1
                self.posx2 = posx2
                self.posy1 = posy1
                self.posy2 = posy2
        def click(self,posx,posy):
                if self.posx1 < posx < self.posx2 and self.posy1 < posy < self.posy2:
                        return True
